   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import lightgbm as lgb\n",
    "import optuna\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.metrics import mean_absolute_error\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "db5a6ecb",
   "metadata": {},
   "outputs": [],
   "source": [
    "# --- Cargar el dataset ENRIQUECIDO ---",
    "DATA_PATH = '../data/processed/featured_player_dataset.csv'\n",
    "df = pd.read_csv(DATA_PATH)\n",
    "\n",
    "# --- Definir Features (X) y Target (y) ---",
    "TARGET = 'market_value_in_eur'\n",
    "cols_to_exclude = [\n",
    "    'player_id', TARGET, 'goals', 'assists', 'minutes_played', 'games_played',\n",
    "    'name', 'position', 'sub_position', 'country_of_citizenship', \n",
    "    'current_club_name', 'contract_expiration_date', 'valuation_date'\n",
    "]\n",
    "features = [col for col in df.columns if col not in cols_to_exclude]\n",
    "\n",
    "X = df[features].select_dtypes(include=np.number)\n",
    "y_log = np.log1p(df[TARGET])\n",
    "\n",
    "# Usaremos un set de validación para el early stopping dentro de la optimización",
    "X_train, X_val, y_train_log, y_val_log = train_test_split(X, y_log, test_size=0.2, random_state=42)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "9aa8d6a6",
   "metadata": {},
   "outputs": [],
   "source": [
    "# =============================================================================",
    "# 1. Definir la Función objective para Optuna",
    "# =============================================================================",
    "# Esta función le dice a Optuna QUÉ optimizar.",
    "# Optuna llamará a esta función muchas veces, cada vez con diferentes 'params'.",
    "\n",
    "def objective(trial):\n",
    "# Definir el espacio de búsqueda de los hiperparámetros",
    "    params = {\n",
    "        'objective': 'regression_l1',  # L1 loss es MAE, perfecto para nuestro objetivo\n",
    "        'metric': 'mae',\n",
    "        'n_estimators': 1000,\n",
    "        'learning_rate': trial.suggest_float('learning_rate', 0.01, 0.3),\n",
    "        'num_leaves': trial.suggest_int('num_leaves', 20, 300),\n",
    "        'max_depth': trial.suggest_int('max_depth', 3, 12),\n",
    "        'min_child_samples': trial.suggest_int('min_child_samples', 5, 100),\n",
    "        'feature_fraction': trial.suggest_float('feature_fraction', 0.4, 1.0),\n",
    "        'bagging_fraction': trial.suggest_float('bagging_fraction', 0.4, 1.0),\n",
    "        'bagging_freq': trial.suggest_int('bagging_freq', 1, 7),\n",
    "        'lambda_l1': trial.suggest_float('lambda_l1', 1e-8, 10.0, log=True),\n",
    "        'lambda_l2': trial.suggest_float('lambda_l2', 1e-8, 10.0, log=True),\n",
    "        'verbose': -1,\n",
    "        'n_jobs': -1,\n",
    "        'seed': 42,\n",
    "    }\n",
    "\n",
    "# Entrenar el model con los parámetros del 'trial' actual",
    "    model = lgb.LGBMRegressor(**params)\n",
    "    \n",
    "# Usar early stopping para no perder tiempo en malos parámetros",
    "    model.fit(X_train, y_train_log,\n",
    "              eval_set=[(X_val, y_val_log)],\n",
    "              eval_metric='mae',\n",
    "              callbacks=[lgb.early_stopping(100, verbose=False)])\n",
    "\n",
    "# Predecir en el set de validación y calcular el error",
    "    preds_log = model.predict(X_val)\n",
    "    mae = mean_absolute_error(y_val_log, preds_log)\n",
    "    \n",
    "    return mae"
   ]
  },
