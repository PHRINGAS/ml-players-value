    "# =============================================================================",
    "# 4. Entrenar el model Final con los MEJORES Parámetros (300 trials) y Evaluar",
    "# =============================================================================",
    "\n",
    "# Usar los mejores parámetros encontrados en la búsqueda de 300 trials",
    "best_params_300 = {\n",
    "    'learning_rate': 0.1963845368531851,\n",
    "    'num_leaves': 226,\n",
    "    'max_depth': 3,\n",
    "    'min_child_samples': 10,\n",
    "    'feature_fraction': 0.5368246241854336,\n",
    "    'bagging_fraction': 0.9625685216118971,\n",
    "    'bagging_freq': 4,\n",
    "    'lambda_l1': 2.52157740846641,\n",
    "    'lambda_l2': 0.04049748399375717\n",
    "}\n",
    "\n",
    "# Añadir parámetros fijos que no estaban en el espacio de búsqueda",
    "best_params_300['objective'] = 'regression_l1'\n",
    "best_params_300['metric'] = 'mae'\n",
    "best_params_300['n_estimators'] = 2000 # Aumentamos los estimadores para el modelo final\n",
    "best_params_300['verbose'] = -1\n",
    "best_params_300['n_jobs'] = -1\n",
    "best_params_300['seed'] = 42\n",
    "\n",
    "print(\"\\n--- Entrenando el modelo FINAL con los parámetros optimizados (300 trials) ---\")\n",
    "\n",
    "# RE-ENTRENAMOS con el set de entrenamiento completo y usamos el de validación para early stopping",
    "final_model_300 = lgb.LGBMRegressor(**best_params_300)\n",
    "\n",
    "final_model_300.fit(X_train, y_train_log,\n",
    "                    eval_set=[(X_val, y_val_log)],\n",
    "                    eval_metric='mae',\n",
    "                    callbacks=[lgb.early_stopping(100, verbose=False)])\n",
    "\n",
    "print(\"Entrenamiento final completado.\")\n",
    "\n",
    "# --- Evaluación Final en el set de validación ---",
    "final_preds_log_300 = final_model_300.predict(X_val)\n",
    "y_val_eur = np.expm1(y_val_log)\n",
    "final_preds_eur_300 = np.expm1(final_preds_log_300)\n",
    "\n",
    "# Calcular métricas finales",
    "mae_final_300 = mean_absolute_error(y_val_eur, final_preds_eur_300)\n",
    "mae_anterior = 3237352\n",
    "baseline_mae = 3982015\n",
    "\n",
    "print(\"\\n--- ¡RESULTADOS FINALES! ---\")\n",
    "print(f\"MAE Baseline (Ridge):              €{baseline_mae:,.0f}\")\n",
    "print(f\"MAE Modelo Anterior (LGBM Default): €{mae_anterior:,.0f}\")\n",
    "print(f\"MAE Modelo Optimizado (LGBM Tuned): €{mae_final_300:,.0f}\")\n",
    "\n",
    "# Calcular la mejora",
    "improvement = mae_anterior - mae_final_300\n",
    "total_improvement = baseline_mae - mae_final_300\n",
    "total_improvement_percent = (total_improvement / baseline_mae) * 100\n",
    "\n",
    "print(\"\\n--- Conclusión de la Optimización ---\")\n",
    "if improvement > 0:\n",
    "    print(f\"¡El modelo optimizado es €{improvement:,.0f} más preciso que el LGBM por defecto!\")\n",
    "    print(f\"La mejora total sobre el baseline es de €{total_improvement:,.0f}, una reducción del error del {total_improvement_percent:.2f}%.\")\n",
    "else:\n",
    "    print(f\"No se ha conseguido una mejora adicional con esta optimización.\")"
   ]
